<div class="waypoint-nav space"></div>
<%= render partial: 'shared/nav' %>

<div class="cart-container">
  <% if @cart.pictures.empty? %>
    <h1 class='center'> Cart Empty </h1>
    <%= link_to "View Prints", prints_path, class: 'center' %>
  <% else %>
    <%= render partial: 'cart_pictures' %>
    <div class="cart-total">
      <%= render partial: 'cart_total' %>
    </div>
  <% end %>
</div>
